// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Example {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model City {
  id              String   @id @default(cuid())
  createdAt       DateTime @default(now())
  city            String
  region          String
  regionCode      String
  population      Int
  averageRent     Int
  walkScore       Int
  transitScore    Int
  bikeScore       Int
  nonViolentCrime Int
  violentCrime    Int
  crimePercentile Int
}
